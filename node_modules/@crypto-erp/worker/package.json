{
  "name": "@crypto-erp/worker",
  "version": "0.1.0",
  "description": "Background worker for Crypto ERP with BullMQ processors",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nest build",
    "dev": "nest start --builder swc --watch",
    "start": "node dist/main.js",
    "start:dev": "nest start --builder swc --watch",
    "start:debug": "nest start --builder swc --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@crypto-erp/database": "*",
    "@nestjs/bullmq": "^10.2.1",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@nestjs/schedule": "^4.1.0",
    "@prisma/client": "^5.20.0",
    "axios": "^1.7.7",
    "bullmq": "^5.16.0",
    "decimal.js": "^10.4.3",
    "ioredis": "^5.4.1",
    "node-forge": "^1.3.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.4",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.3",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
